{
  "name": "dracor-frontend",
  "version": "0.24.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": ">=4.3.1",
    "chart.js": "^2.7.1",
    "classnames": "^2.2.5",
    "http-proxy-middleware": "^0.19.1",
    "jsnetworkx": "fkling/JSNetworkX#v0.3.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-octicon": "^3.0.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.1.8",
    "react-sigma": "^1.2.29",
    "reactable": "dracor-org/reactable",
    "reactstrap": "5.0.0-alpha.4",
    "swagger-ui-react": "^3.21.0",
    "whatwg-fetch": "^2.0.3",
    "yasgui": "^2.7.29"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deployment": "yarn run build && rsync -av build/ root@dracor.org:/var/www/html/"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.11.0",
    "eslint-config-xo-react": "^0.15.0",
    "eslint-plugin-react": "^7.5.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "extends": "xo-react",
    "space": true,
    "semicolon": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "space-before-function-paren": [
        "warn",
        "always"
      ],
      "capitalized-comments": 0,
      "func-names": 0,
      "max-params": 0,
      "spaced-comment": 0,
      "import/no-unassigned-import": 0,
      "operator-linebreak": [
        1,
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before"
          }
        }
      ],
      "react/destructuring-assignment": [
        1,
        "always"
      ],
      "react/forbid-component-props": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-space-before-closing": 0,
      "react/jsx-tag-spacing": 1,
      "react/prop-types": [
        1,
        {
          "ignore": []
        }
      ],
      "unicorn/filename-case": 0
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
